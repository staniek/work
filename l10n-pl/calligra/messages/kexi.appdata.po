#: kexi.appdata.xml:15
msgid "Features:"
msgstr "Możliwości:"

#. (itstool) path: ul/li
#: kexi.appdata.xml:17
msgid "Full visual designers for tables, queries, forms and reports"
msgstr ""
"W pełni wizualna możliwość tworzenia tabel, zapytań, formularzy i sprawozdań"

#. (itstool) path: ul/li
#: kexi.appdata.xml:18
msgid "Running queries, support for parametrized queries"
msgstr "Wykonywanie zapytań, obsługa dla zapytań parametrycznych"

#. (itstool) path: ul/li
#: kexi.appdata.xml:19
